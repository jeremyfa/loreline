
state
  visits: 0

beat Main
  visits += 1
  choice
    Direct
      cycle
        "First visit direct"
      --
        "Return visit direct"
    + Inserted

beat Inserted
  choice
    Pick
      cycle
        "First visit pick"
      --
        "Return visit pick"

