
state
  total: 0

beat Start
  Visit()
  Visit()
  Visit()
  Visit()
  Grand total: $total

beat Visit
  cycle
    total += 10
    Added 10, total is $total
  --
    total += 20
    Added 20, total is $total
  --
    total += 5
    Added 5, total is $total

