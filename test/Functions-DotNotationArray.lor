
function testArrayDotNotation()
  var items = [3, 1, 2]
  items.sort()
  var sorted = items.join(", ")
  items.add(4)
  var added = items.join(", ")
  var hasTwo = items.has(2)
  var idx = items.index(3)
  var popped = items.pop()
  var afterPop = items.join(", ")
  items.prepend(0)
  var afterPrepend = items.join(", ")
  var shifted = items.shift()
  var afterShift = items.join(", ")
  var copied = items.copy()
  copied.reverse()
  var len = items.length()
  return "sorted: $sorted | added: $added | has 2: $hasTwo | index of 3: $idx | popped: $popped | after pop: $afterPop | prepend 0: $afterPrepend | shifted: $shifted | after shift: $afterShift | reversed copy: " + copied.join(", ") + " | length: $len"

$testArrayDotNotation()

